#Mainprogramm.start
#DRIVER
#SAFE

// Driver definition
<drv.
Const.driver = keyboard_driver
keyboard_driver <<func = keyboard>>
.dr>

// Main program section
<.de
    var key: i32 = 0
    var msg: string = "Press a key"
    
    static.pl>
    
    display{msg}
    
    // Call driver
    call #keyboard_driver
    
    display{key}
    
    free{key}
    free{msg}
.>

#Mainprogramm.end
#DRIVER.stop
